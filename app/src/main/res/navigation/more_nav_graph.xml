<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/more_nav_graph"
    app:startDestination="@id/moreFragment">

    <fragment
        android:id="@+id/moreFragment"
        android:name="com.example.bottomnavigationviewproject.MoreFragment"
        android:label="More"
        tools:layout="@layout/fragment_more" >
        <action
            android:id="@+id/action_moreFragment_to_moreDetailsFragment"
            app:destination="@id/moreDetailsFragment" />
    </fragment>
    <fragment
        android:id="@+id/moreDetailsFragment"
        android:name="com.example.bottomnavigationviewproject.MoreDetailsFragment"
        android:label="MoreDetails"
        tools:layout="@layout/fragment_more_details" />
</navigation>